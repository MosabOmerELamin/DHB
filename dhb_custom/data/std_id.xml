<?xml version="1.0" encoding="utf-8"?>
<odoo >
    <data >
        <!-- Define sequence for serial_id -->
        <record id="sequence_tc_student_serial" model="ir.sequence">
            <field name="name">Tc Student Serial</field>
            <field name="code">tc.student.serial</field>
            <field name="prefix">STU</field> <!-- You can customize the prefix as per your requirement -->
            <field name="padding">5</field> <!-- You can customize the padding as per your requirement -->
            <field name="number_increment">1</field>
        </record>
    </data>
</odoo>    